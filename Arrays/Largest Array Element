#include<bits/stdc++.h>
using namespace std;
int largest_arr_elem(int arr[], int n){
    int maxv=arr[0];
    for(int i=0;i<n;i++){
        maxv=max(maxv, arr[i]);
    }
    return maxv;
}
int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    cout<<largest_arr_elem(arr, n);
}
