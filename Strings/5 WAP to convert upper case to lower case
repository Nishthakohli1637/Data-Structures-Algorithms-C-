#include<bits/stdc++.h>
using namespace std;
int main(){
    string str;
    cin>>str;
    int n=str.size();
    for(int i=0;i<n;i++){
        if(isupper(str[i])){
            str[i]=tolower(str[i]);
        }
    }
    cout<<str;
}
// Without using stl

#include<bits/stdc++.h>
using namespace std;
int main(){
    string str;
    cin>>str;
    int n=str.size();
    for(int i=0;i<n;i++){
        if(isupper(str[i])){
            str[i]=str[i]-'A'+'a';
        }
    }
    cout<<str;
}
