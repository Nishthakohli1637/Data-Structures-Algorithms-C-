#include<bits/stdc++.h>
using namespace std;
int main(){
    string str;
    cin>>str;
    int n=str.size();
    for(int i=0;i<n;i++){
    if(islower(str[i])){
        str[i]=toupper(str[i]);
    }
    }
    cout<<str;
}

//without using strl

#include<bits/stdc++.h>
using namespace std;
int main(){
    string str;
    cin>>str;
    int n=str.size();
    for(int i=0;i<n;i++){
    if(islower(str[i])){
       str[i]=str[i]-'a'+'A';
    }
    }
    cout<<str;
}
