class Solution {
public:
    bool hasCycle(ListNode *head) {
        if(head==NULL || head->next==NULL){
            return false;
        }
        ListNode *p=head;
        ListNode *q=head;
    while(p && q){
            p=p->next;
            q=q->next;
            if(q){
                q=q->next;
            }
            if(p==q){
            return true;
        }
        }
        return false;
    }
};
